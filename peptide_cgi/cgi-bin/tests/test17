#! /bin/csh -f

# This (along with test18) is to test the 100% (NMR Browser) vs. 97%
# (Jafar) cutoff for sequence identity for PDB coordinates.

set result = 0

rm -f */test17_out*

echo -n "Processing NMR-STAR: "
./s2d -nomail -file test_data/bmr4267mod -name test17_out -do_pdb 1 -force \
  -check_res_list -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif

echo "Checking output files"

set files = (data_dir/test17_outas1.dat \
	data_dir/test17_outc1.dat \
	data_dir/test17_outd1.dat \
	data_dir/test17_outg1.dat \
	data_dir/test17_outhn1.dat \
	data_dir/test17_outmd.dat \
	data_dir/test17_outn1.dat \
	data_dir/test17_outn2.dat \
	data_dir/test17_outn3.dat \
	data_dir/test17_outn4.dat \
	data_dir/test17_outn5.dat \
	data_dir/test17_outn6.dat \
	data_dir/test17_outn7.dat \
	data_dir/test17_outn8.dat \
	data_dir/test17_outn9.dat \
	data_dir/test17_outp1.dat \
	data_dir/test17_outrc1.dat \
	data_dir/test17_outrl.dat \
	data_dir/test17_outt11.dat \
	data_dir/test17_outt12.dat \
	data_dir/test17_outt13.dat \
	data_dir/test17_outt21.dat \
	data_dir/test17_outt22.dat \
	data_dir/test17_outt23.dat \
	html_dir/test17_outas1.html \
	html_dir/test17_outc1.html \
	html_dir/test17_outd1.html \
	html_dir/test17_outg1.html \
	html_dir/test17_outhn1.html \
	html_dir/test17_outn1.html \
	html_dir/test17_outn2.html \
	html_dir/test17_outn3.html \
	html_dir/test17_outn4.html \
	html_dir/test17_outn5.html \
	html_dir/test17_outn6.html \
	html_dir/test17_outn7.html \
	html_dir/test17_outn8.html \
	html_dir/test17_outn9.html \
	html_dir/test17_outp1.html \
	html_dir/test17_outt11.html \
	html_dir/test17_outt12.html \
	html_dir/test17_outt13.html \
	html_dir/test17_outt21.html \
	html_dir/test17_outt22.html \
	html_dir/test17_outt23.html \
	html_dir/test17_outy.html \
	html_dir/test17_outy_large.html \
	session_dir/test17_outacdd \
	session_dir/test17_outas1.ds \
	session_dir/test17_outc1.ds \
	session_dir/test17_outd1.ds \
	session_dir/test17_outg1.ds \
	session_dir/test17_outhn1.ds \
	session_dir/test17_outn1.ds \
	session_dir/test17_outn2.ds \
	session_dir/test17_outn3.ds \
	session_dir/test17_outn4.ds \
	session_dir/test17_outn5.ds \
	session_dir/test17_outn6.ds \
	session_dir/test17_outn7.ds \
	session_dir/test17_outn8.ds \
	session_dir/test17_outn9.ds \
	session_dir/test17_outp1.ds \
	session_dir/test17_outt11.ds \
	session_dir/test17_outt12.ds \
	session_dir/test17_outt13.ds \
	session_dir/test17_outt21.ds \
	session_dir/test17_outt22.ds \
	session_dir/test17_outt23.ds \
	data_dir/test17_outam1.dat \
	html_dir/test17_outam1.html \
	session_dir/test17_outam1.ds \
	html_dir/test17_outam1_large.html \
	html_dir/test17_outas1_large.html \
	html_dir/test17_outc1_large.html \
	html_dir/test17_outd1_large.html \
	html_dir/test17_outg1_large.html \
	html_dir/test17_outhn1_large.html \
	html_dir/test17_outn1_large.html \
	html_dir/test17_outn2_large.html \
	html_dir/test17_outn3_large.html \
	html_dir/test17_outn4_large.html \
	html_dir/test17_outn5_large.html \
	html_dir/test17_outn6_large.html \
	html_dir/test17_outn7_large.html \
	html_dir/test17_outn8_large.html \
	html_dir/test17_outn9_large.html \
	html_dir/test17_outp1_large.html \
	html_dir/test17_outt11_large.html \
	html_dir/test17_outt12_large.html \
	html_dir/test17_outt13_large.html \
	html_dir/test17_outt21_large.html \
	html_dir/test17_outt22_large.html \
	html_dir/test17_outt23_large.html)

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

# Check for correct PDB links in summary html file.
if (`grep -l 1QQS html_dir/test17_outy.html` != "") then
  echo "Error: PDB ID 1QQS should have been rejected because"
  echo "  sequence identity is less than 100%."
  set result = 1
endif

if (`grep -l 1DFV html_dir/test17_outy.html` != "") then
  echo "Error: PDB ID 1DFV should have been rejected because"
  echo "  the number of residues does not match."
  set result = 1
endif

if (`grep -l 1NGL html_dir/test17_outy.html` == "") then
  echo "Error: PDB ID 1NGL should have been linked."
  set result = 1
endif

if (`grep -l 1L6M html_dir/test17_outy.html` != "") then
  echo "Error: PDB ID 1L6M should have been rejected because"
  echo "  sequence identity is less than 100%."
  set result = 1
endif

echo -n "Test17 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
