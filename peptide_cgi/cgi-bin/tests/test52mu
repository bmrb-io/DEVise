#! /bin/csh -f

# Tests the starlibj fix for the visualization server problem that
# Hongyang ran into; this also tests the make_uvd script.

set result = 0

rm -f */*/test52_out*
rm -f */.*/test52_out*

echo -n "Processing NMR-STAR: "
./make_uvd file=test_data/20080425_38208.str\&name=test52_out
if ($status != 0) then
  echo "First invocation of uvd failed!"
  set result = 1
endif


echo "Checking output files"

set files = ( data_dir/uvd/test52_outam1.dat \
	data_dir/uvd/test52_outamac1.dat \
	data_dir/uvd/test52_outas1.dat \
	data_dir/uvd/test52_outc1.dat \
	data_dir/uvd/test52_outd1.dat \
	data_dir/uvd/test52_outhc1.dat \
	data_dir/uvd/test52_outhn1.dat \
	data_dir/uvd/test52_outmd.dat \
	data_dir/uvd/test52_outp1.dat \
	data_dir/uvd/test52_outrc1.dat \
	data_dir/uvd/test52_outrl.dat \
	html_dir/uvd/test52_outam1.html \
	html_dir/uvd/test52_outam1_large.html \
	html_dir/uvd/test52_outas1.html \
	html_dir/uvd/test52_outas1_large.html \
	html_dir/uvd/test52_outc1.html \
	html_dir/uvd/test52_outc1_large.html \
	html_dir/uvd/test52_outd1.html \
	html_dir/uvd/test52_outd1_large.html \
	html_dir/uvd/test52_outhc1.html \
	html_dir/uvd/test52_outhc1_large.html \
	html_dir/uvd/test52_outhn1.html \
	html_dir/uvd/test52_outhn1_large.html \
	html_dir/uvd/test52_outp1.html \
	html_dir/uvd/test52_outp1_large.html \
	html_dir/uvd/test52_outy.html \
	html_dir/uvd/test52_outy_large.html \
	session_dir/.uvd/test52_outacdd \
	session_dir/.uvd/test52_outam1.ds \
	session_dir/.uvd/test52_outas1.ds \
	session_dir/.uvd/test52_outc1.ds \
	session_dir/.uvd/test52_outd1.ds \
	session_dir/.uvd/test52_outhc1.ds \
	session_dir/.uvd/test52_outhn1.ds \
	session_dir/.uvd/test52_outp1.ds )
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test52mu result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
