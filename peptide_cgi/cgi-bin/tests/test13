#! /bin/csh -f

# Tests visualization-server mode -- local NMR-STAR file, but will try
# to get and process data from PDB.  Note: this NMR-STAR file is one
# assembled by Jafar from various uploaded pieces.

set result = 0

rm -f */test13_out*
rm -f */*/test13_out*

echo -n "Processing NMR-STAR and mmCIF: "
#TEMPTEMP -- -check_res_list makes this fail -- should fix input file.
./vsc -file test_data/Jafar1.str -name test13_out -do_csr 2 -force -check_pct
if ($status != 0) then
  echo "First invocation of vsc failed!"
  set result = 1
endif


echo "Checking output files"

set files = (data_dir/vis_server/test13_outas1.dat \
	data_dir/vis_server/test13_outc1.dat \
	data_dir/vis_server/test13_outd1.dat \
	data_dir/vis_server/test13_outg1.dat \
	data_dir/vis_server/test13_outhn1.dat \
	data_dir/vis_server/test13_outmd.dat \
	data_dir/vis_server/test13_outp1.dat \
	data_dir/vis_server/test13_outrc1.dat \
	data_dir/vis_server/test13_outrl1.dat \
	data_dir/vis_server/test13_outt11.dat \
	data_dir/vis_server/test13_outt12.dat \
	data_dir/vis_server/test13_outt13.dat \
	data_dir/vis_server/test13_outt21.dat \
	data_dir/vis_server/test13_outt22.dat \
	data_dir/vis_server/test13_outt23.dat \
	html_dir/test13_outas1.html \
	html_dir/test13_outc1.html \
	html_dir/test13_outd1.html \
	html_dir/test13_outg1.html \
	html_dir/test13_outhn1.html \
	html_dir/test13_outp1.html \
	html_dir/test13_outt11.html \
	html_dir/test13_outt12.html \
	html_dir/test13_outt13.html \
	html_dir/test13_outt21.html \
	html_dir/test13_outt22.html \
	html_dir/test13_outt23.html \
	html_dir/test13_outy.html \
	html_dir/test13_outy_large.html \
	session_dir/vis_server/test13_outacdd \
	session_dir/vis_server/test13_outas1.ds \
	session_dir/vis_server/test13_outc1.ds \
	session_dir/vis_server/test13_outd1.ds \
	session_dir/vis_server/test13_outg1.ds \
	session_dir/vis_server/test13_outhn1.ds \
	session_dir/vis_server/test13_outp1.ds \
	session_dir/vis_server/test13_outt11.ds \
	session_dir/vis_server/test13_outt12.ds \
	session_dir/vis_server/test13_outt13.ds \
	session_dir/vis_server/test13_outt21.ds \
	session_dir/vis_server/test13_outt22.ds \
	session_dir/vis_server/test13_outt23.ds \
	html_dir/test13_outam1.html \
	data_dir/vis_server/test13_outam1.dat \
	session_dir/vis_server/test13_outam1.ds \
	html_dir/test13_outam1_large.html \
	html_dir/test13_outas1_large.html \
	html_dir/test13_outc1_large.html \
	html_dir/test13_outd1_large.html \
	html_dir/test13_outg1_large.html \
	html_dir/test13_outhn1_large.html \
	html_dir/test13_outp1_large.html \
	html_dir/test13_outt11_large.html \
	html_dir/test13_outt12_large.html \
	html_dir/test13_outt13_large.html \
	html_dir/test13_outt21_large.html \
	html_dir/test13_outt22_large.html \
	html_dir/test13_outt23_large.html)
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test13 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
