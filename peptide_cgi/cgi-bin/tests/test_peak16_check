echo "Checking output files"

set files = ( 15350_peaks.str )

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

set file = "15350_peaks.str"
set line = '^save_spectral_peak_list_1'
source tests/check4line
