#! /bin/csh -f

#TEMPTEMP -- comment is wrong??
# Tests visualization-server mode with local bmr4096 (has coordinates).

set result = 0

rm */test8_out* >& /dev/null
rm */*/test8_out* >& /dev/null

echo -n "Processing NMR-STAR: "
./vsc -file ../cgi-bin/test_data/bmr4081.str -name test8_out -force \
  -check_res_list -check_pct
if ($status != 0) then
  echo "First invocation of vsc failed!"
  set result = 1
endif


echo "Checking output files"

#TEMPTEMP
set files = ()
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test8 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
