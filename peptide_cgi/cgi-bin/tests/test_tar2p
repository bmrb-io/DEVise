#! /bin/csh -f

# Hopefully final phase of torsion angle processing -- just process
# by using the PDB ID -- need to send request to the restraint grid
# and parse the result.  (This version tests processing everything
# at one shot.)

set result = 0

rm -f */16270*

echo -n "Processing NMR-STAR, mmCIF and torsion angle restraints: "
./s2d -nomail -bmrbid 16270 -do_tar 3 -force -check_res_list -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


source tests/test_tar2_check


echo -n "Test_tar2p result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
