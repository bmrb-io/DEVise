#! /bin/csh -f

# Generate a LACS visualization that won't get overwritten by
# "normal" processing.

set result = 0

rm -f */lacs_example1*

echo -n "Processing NMR-STAR: "
./s2d -nomail -bmrbid 4267 -name lacs_example1 -force -check_res_list \
  -lacs_file test_data/bmr4267_LACS.out
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


echo "Checking output files"

set files = (data_dir/lacs_example1as1.dat data_dir/lacs_example1c1.dat \
	data_dir/lacs_example1d1.dat data_dir/lacs_example1g1.dat data_dir/lacs_example1hn1.dat \
	data_dir/lacs_example1md.dat data_dir/lacs_example1n1.dat data_dir/lacs_example1n2.dat \
	data_dir/lacs_example1n3.dat data_dir/lacs_example1n4.dat data_dir/lacs_example1n5.dat \
	data_dir/lacs_example1n6.dat data_dir/lacs_example1n7.dat data_dir/lacs_example1n8.dat \
	data_dir/lacs_example1n9.dat data_dir/lacs_example1p1.dat data_dir/lacs_example1rc1.dat \
	data_dir/lacs_example1rl1.dat data_dir/lacs_example1t11.dat data_dir/lacs_example1t12.dat \
	data_dir/lacs_example1t13.dat data_dir/lacs_example1t21.dat data_dir/lacs_example1t22.dat \
	data_dir/lacs_example1t23.dat html_dir/lacs_example1as1.html \
	html_dir/lacs_example1c1.html html_dir/lacs_example1d1.html html_dir/lacs_example1g1.html \
	html_dir/lacs_example1hn1.html html_dir/lacs_example1n1.html html_dir/lacs_example1n2.html \
	html_dir/lacs_example1n3.html html_dir/lacs_example1n4.html html_dir/lacs_example1n5.html \
	html_dir/lacs_example1n6.html html_dir/lacs_example1n7.html html_dir/lacs_example1n8.html \
	html_dir/lacs_example1n9.html html_dir/lacs_example1p1.html html_dir/lacs_example1t11.html \
	html_dir/lacs_example1t12.html html_dir/lacs_example1t13.html html_dir/lacs_example1t21.html \
	html_dir/lacs_example1t22.html html_dir/lacs_example1t23.html \
	html_dir/lacs_example1y.html session_dir/lacs_example1acdd \
	session_dir/lacs_example1as1.ds session_dir/lacs_example1c1.ds session_dir/lacs_example1d1.ds \
	session_dir/lacs_example1g1.ds session_dir/lacs_example1hn1.ds session_dir/lacs_example1n1.ds \
	session_dir/lacs_example1n2.ds session_dir/lacs_example1n3.ds session_dir/lacs_example1n4.ds \
	session_dir/lacs_example1n5.ds session_dir/lacs_example1n6.ds session_dir/lacs_example1n7.ds \
	session_dir/lacs_example1n8.ds session_dir/lacs_example1n9.ds session_dir/lacs_example1p1.ds \
	session_dir/lacs_example1t11.ds session_dir/lacs_example1t12.ds session_dir/lacs_example1t13.ds \
	session_dir/lacs_example1t21.ds session_dir/lacs_example1t22.ds session_dir/lacs_example1t23.ds \
	data_dir/lacs_example1am1.dat \
	data_dir/lacs_example1amac1.dat \
	html_dir/lacs_example1am1.html \
	session_dir/lacs_example1am1.ds \
	data_dir/lacs_example1lacsc1.dat \
	data_dir/lacs_example1lacsc2.dat \
	data_dir/lacs_example1lacsc3.dat \
	data_dir/lacs_example1lacsc4.dat \
	data_dir/lacs_example1lacsl1.dat \
	data_dir/lacs_example1lacsl2.dat \
	data_dir/lacs_example1lacsl3.dat \
	data_dir/lacs_example1lacsl4.dat \
	data_dir/lacs_example1lacsp1.dat \
	data_dir/lacs_example1lacsp2.dat \
	data_dir/lacs_example1lacsp3.dat \
	data_dir/lacs_example1lacsp4.dat \
	html_dir/lacs_example1lacs1.html \
	html_dir/lacs_example1lacs2.html \
	html_dir/lacs_example1lacs3.html \
	html_dir/lacs_example1lacs4.html \
	session_dir/lacs_example1lacs1.ds \
	session_dir/lacs_example1lacs2.ds \
	session_dir/lacs_example1lacs3.ds \
	session_dir/lacs_example1lacs4.ds)

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test36 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
