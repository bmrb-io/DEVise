#! /bin/csh -f

# Tests visualization of SPARTA-calculated theoretical deltashifts
# (NMR-STAR 3.1 file).

set result = 0

rm -f */16350*

echo -n "Processing NMR-STAR: "
./s2d -file test_data/sd16350_3.str -name 16350 -force -check_res_list \
  -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


echo "Checking output files"

set files = ( data_dir/16350am1.dat \
	data_dir/16350amac1.dat \
	data_dir/16350as1.dat \
	data_dir/16350c1.dat \
	data_dir/16350d1.dat \
	data_dir/16350hc1.dat \
	data_dir/16350hn1.dat \
	data_dir/16350md.dat \
	data_dir/16350p1.dat \
	data_dir/16350rc1.dat \
	data_dir/16350rl.dat \
	data_dir/16350sd1.dat \
	html_dir/16350am1.html \
	html_dir/16350am1_large.html \
	html_dir/16350as1.html \
	html_dir/16350as1_large.html \
	html_dir/16350c1.html \
	html_dir/16350c1_large.html \
	html_dir/16350d1.html \
	html_dir/16350d1_large.html \
	html_dir/16350hc1.html \
	html_dir/16350hc1_large.html \
	html_dir/16350p1.html \
	html_dir/16350p1_large.html \
	html_dir/16350sd1.html \
	html_dir/16350sd1_large.html \
	html_dir/16350y.html \
	html_dir/16350y_large.html \
	session_dir/16350acdd \
	session_dir/16350am1.ds \
	session_dir/16350as1.ds \
	session_dir/16350c1.ds \
	session_dir/16350d1.ds \
	session_dir/16350hc1.ds \
	session_dir/16350p1.ds \
	session_dir/16350sd1.ds )
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


set file = "data_dir/16350d1.dat"
set line = '^1 1 TYR -0.761 0.0 -0.942 0.497$'
source tests/check4line
set line = '^1 61 GLU -0.178 0.0 0.985 0.946$'
source tests/check4line

set file = "data_dir/16350sd1.dat"
set line = '^1 1 2 ASN -0.098 0.0 0.594 0.0 0.0 -0.641$'
source tests/check4line
set line = '^1 1 60 MET 0.228 0.0 -3.258 0.0 0.0 -0.437$'
source tests/check4line

set file = "html_dir/16350y.html"
set line = '^<p><b>SPARTA-calculated chemical shift deltas</b></p><table border>$'
source tests/check4line
set line = '^    <td><a href="16350sd1.html">59 residues</a></td>$'
source tests/check4line

set file = "html_dir/16350y_large.html"
set line = '^<p><b>SPARTA-calculated chemical shift deltas</b></p><table border>$'
source tests/check4line
set line = '^    <td><a href="16350sd1_large.html">59 residues</a></td>$'
source tests/check4line


echo -n "Test_sparta2 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
