#! /bin/csh -f

# Test the latest NMR-STAR 3.0.

set result = 0

rm -r -f */6722*

echo -n "Processing NMR-STAR: "
./s2d -nomail -file test_data/bmr6722.str.nmrstr -name 6722 -force \
  -check_res_list -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


echo "Checking output files"

set files = (data_dir/6722am1.dat \
	data_dir/6722amac1.dat \
	data_dir/6722as1.dat \
	data_dir/6722d1.dat \
	data_dir/6722hn1.dat \
	data_dir/6722md.dat \
	data_dir/6722p1.dat \
	data_dir/6722rc1.dat \
	data_dir/6722rl.dat \
	html_dir/6722am1.html \
	html_dir/6722as1.html \
	html_dir/6722d1.html \
	html_dir/6722hn1.html \
	html_dir/6722p1.html \
	html_dir/6722y.html \
	html_dir/6722y_large.html \
	session_dir/6722/6722acdd \
	session_dir/6722/6722am1.ds \
	session_dir/6722/6722as1.ds \
	session_dir/6722/6722d1.ds \
	session_dir/6722/6722hn1.ds \
	session_dir/6722/6722p1.ds \
	html_dir/6722am1_large.html \
	html_dir/6722as1_large.html \
	html_dir/6722d1_large.html \
	html_dir/6722hn1_large.html \
	html_dir/6722p1_large.html )

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test33 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
