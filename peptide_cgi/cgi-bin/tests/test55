#! /bin/csh -f

# Tests the problem with 6730 and others that David Miller reported
# (bug 069).

set result = 0

rm -f */6730*

echo -n "Processing NMR-STAR: "
./s2d -bmrbid 6730 -force -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


echo "Checking output files"

set files = ( data_dir/6730as1.dat \
	data_dir/6730c1.dat \
	data_dir/6730d1.dat \
	data_dir/6730hn1.dat \
	data_dir/6730md.dat \
	data_dir/6730p1.dat \
	data_dir/6730rc1.dat \
	data_dir/6730rl.dat \
	html_dir/6730as1.html \
	html_dir/6730as1_large.html \
	html_dir/6730c1.html \
	html_dir/6730c1_large.html \
	html_dir/6730d1.html \
	html_dir/6730d1_large.html \
	html_dir/6730hn1.html \
	html_dir/6730hn1_large.html \
	html_dir/6730p1.html \
	html_dir/6730p1_large.html \
	html_dir/6730y.html \
	html_dir/6730y_large.html \
	session_dir/6730acdd \
	session_dir/6730as1.ds \
	session_dir/6730c1.ds \
	session_dir/6730d1.ds \
	session_dir/6730hn1.ds \
	session_dir/6730p1.ds )
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test55 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
