#! /bin/csh -f

# Initial test for processing unreleased BMRB entries.

set result = 0

rm -f */4081ur*

echo -n "Processing NMR-STAR and mmCIF: "
./vur -file test_data/bmr4081.str -name 4081ur \
  -lacs_file test_data/bmr4081_LACS.str
if ($status != 0) then
  echo "First invocation of vur failed!"
  set result = 1
endif


echo "Checking output files"

set files = ( data_dir/4081urac1.dat \
	data_dir/4081uram1.dat \
	data_dir/4081uramac1.dat \
	data_dir/4081uras1.dat \
	data_dir/4081urc1.dat \
	data_dir/4081urd1.dat \
	data_dir/4081urhn1.dat \
	data_dir/4081urmd.dat \
	data_dir/4081urp1.dat \
	data_dir/4081urrc1.dat \
	data_dir/4081urrl1.dat \
	html_unreleased/4081urac1.html \
	html_unreleased/4081uram1.html \
	html_unreleased/4081uras1.html \
	html_unreleased/4081urc1.html \
	html_unreleased/4081urd1.html \
	html_unreleased/4081urhn1.html \
	html_unreleased/4081urp1.html \
	html_unreleased/4081ury.html \
	session_dir/.unreleased/4081urac1.ds \
	session_dir/.unreleased/4081uracdd \
	session_dir/.unreleased/4081uram1.ds \
	session_dir/.unreleased/4081uras1.ds \
	session_dir/.unreleased/4081urc1.ds \
	session_dir/.unreleased/4081urd1.ds \
	session_dir/.unreleased/4081urhn1.ds \
	session_dir/.unreleased/4081urp1.ds \
	data_dir/4081urlacsc1.dat \
	data_dir/4081urlacsc2.dat \
	data_dir/4081urlacsc3.dat \
	data_dir/4081urlacsc4.dat \
	data_dir/4081urlacsl1.dat \
	data_dir/4081urlacsl2.dat \
	data_dir/4081urlacsl3.dat \
	data_dir/4081urlacsl4.dat \
	data_dir/4081urlacsp1.dat \
	data_dir/4081urlacsp2.dat \
	data_dir/4081urlacsp3.dat \
	data_dir/4081urlacsp4.dat \
	html_unreleased/4081urlacs1.html \
	html_unreleased/4081urlacs2.html \
	html_unreleased/4081urlacs3.html \
	html_unreleased/4081urlacs4.html)

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

echo -n "Test32 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
