	session_dir/11017/11017p1.ds \
	session_dir/11017/11017sd1.ds )

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


set file = "data_dir/11017d1.dat"
set line = '^1 2 THR 0.01 -0.9 -1.2 1.8$'
