#! /bin/csh -f

# Pretty much "normal" processing except that we're doing both the BMRB
# file and the PDB file in a single step (both from the web).
# This tests bug 028.
# Note: we're also testing that with do_lacs 1 we don't fail overall
# even when we can't find the LACS file.

set result = 0

rm -r -f */4867*

echo -n "Processing NMR-STAR and mmCIF: "
./s2d -nomail -bmrbid 4867 -do_pdb 2 -force -check_res_list \
  -do_lacs 1 -lacs_url file:test_data/ -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


source tests/test14_check

# The 2.1 and 3.1 files have slightly different sample data.
set file = "data_dir/4867md.dat"
set line = 'EA 2 (polypeptide(L))	2	HA delta chem shift \[2\]	-	sNRE: 1 mM; RBD12: 1 mM	pH: 6.2; temperature: 303 K; ionic strength: 150 mM	4867d1	\$HA_DeltaShift$'
source tests/check4line
set line = 'EA 2 (polypeptide(L))	2	% 15N assign per res \[2\]	-	sNRE: 1 mM; RBD12: 1 mM	pH: 6.2; temperature: 303 K; ionic strength: 150 mM	4867p1	\$assigForN$'
source tests/check4line
set line = 'EA 1 (RNA)	1	N3 delta chem shift \[1\]	-	sNRE: 1 mM; RBD12: 1 mM; D2O: 100 %	pH: 6.2; temperature: 303 K; ionic strength: 150 mM	4867d2	\$N3_DeltaShift$'
source tests/check4line

echo -n "Test14 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
