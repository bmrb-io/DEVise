#! /bin/csh -f

# Tests "upload and visualize" mode.

set result = 0

set noleech = 0

if ($#argv > 0) then
  if (xx$1 == xx-noleech) then
    set noleech = 1
  endif
endif

rm -f */*/test47_out*
rm -f */.*/test47_out*

echo -n "Processing NMR-STAR: "
# This is 15011.
./uvd -file test_data/20070228.28103.str -name test47_out -force -do_csr 1 \
  -check_pct
if ($status != 0) then
  echo "First invocation of uvd failed!"
  set result = 1
endif
mv tmp/uvd.out tmp/uvd.out1

echo -n "Processing NMR-STAR and mmCIF: "
#TEMP -- have this call make_uvd?
./uvd -file test_data/20070228.28103.str -name test47_out -force -pdbid 1FSP -do_pdb 2 -coord_index 1 -check_pct
if ($status != 0) then
  echo "Second invocation of uvd failed!"
  set result = 1
endif
mv tmp/uvd.out tmp/uvd.out2

echo -n "Processing NMR-STAR and chemical shift reference: "
#TEMP -- have this call make_uvd?
if ($noleech) then
  tests/test47_noleech
  if ($status) then
    set result = 1
  endif
else
  tests/test47_main
  if ($status) then
    set result = 1
  endif
endif

echo "Checking output files"

set files = ( data_dir/uvd/test47_outac1.dat \
	data_dir/uvd/test47_outam1.dat \
	data_dir/uvd/test47_outamac1.dat \
	data_dir/uvd/test47_outas1.dat \
	data_dir/uvd/test47_outc1.dat \
	data_dir/uvd/test47_outd1.dat \
	data_dir/uvd/test47_outhc1.dat \
	data_dir/uvd/test47_outhn1.dat \
	data_dir/uvd/test47_outmd.dat \
	data_dir/uvd/test47_outp1.dat \
	data_dir/uvd/test47_outrc1.dat \
	data_dir/uvd/test47_outrl.dat \
	html_dir/uvd/test47_outac1.html \
	html_dir/uvd/test47_outac1_large.html \
	html_dir/uvd/test47_outam1.html \
	html_dir/uvd/test47_outam1_large.html \
	html_dir/uvd/test47_outas1.html \
	html_dir/uvd/test47_outas1_large.html \
	html_dir/uvd/test47_outc1.html \
	html_dir/uvd/test47_outc1_large.html \
	html_dir/uvd/test47_outd1.html \
	html_dir/uvd/test47_outd1_large.html \
	html_dir/uvd/test47_outhc1.html \
	html_dir/uvd/test47_outhc1_large.html \
	html_dir/uvd/test47_outhn1.html \
	html_dir/uvd/test47_outhn1_large.html \
	html_dir/uvd/test47_outp1.html \
	html_dir/uvd/test47_outp1_large.html \
	html_dir/uvd/test47_outtmp1y.html \
	html_dir/uvd/test47_outtmp1y_large.html \
	html_dir/uvd/test47_outy.html \
	html_dir/uvd/test47_outy_large.html \
	session_dir/.uvd/test47_outac1.ds \
	session_dir/.uvd/test47_outacdd \
	session_dir/.uvd/test47_outam1.ds \
	session_dir/.uvd/test47_outas1.ds \
	session_dir/.uvd/test47_outc1.ds \
	session_dir/.uvd/test47_outd1.ds \
	session_dir/.uvd/test47_outhc1.ds \
	session_dir/.uvd/test47_outhn1.ds \
	session_dir/.uvd/test47_outp1.ds \
	data_dir/uvd/test47_outcsr1_1.dat \
	data_dir/uvd/test47_outcsr1_2.dat \
	data_dir/uvd/test47_outcsr1_3.dat \
	data_dir/uvd/test47_outcsr1_4.dat \
	data_dir/uvd/test47_outcsr1_5.dat \
	data_dir/uvd/test47_outcsr1src.dat \
	html_dir/uvd/test47_outcsr11.html \
	html_dir/uvd/test47_outcsr11_large.html \
	html_dir/uvd/test47_outcsr21.html \
	html_dir/uvd/test47_outcsr21_large.html \
	html_dir/uvd/test47_outcsr31.html \
	html_dir/uvd/test47_outcsr31_large.html \
	session_dir/.uvd/test47_outcsr11.ds \
	session_dir/.uvd/test47_outcsr21.ds \
	session_dir/.uvd/test47_outcsr31.ds )
	
foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

# Check for correct structure link in summary html file.
if (`grep -l 'make_uvd?pdbid=1FSP&file=test_data/20070228.28103.str&name=test47_out&do_pdb=2&coord_index=1' html_dir/uvd/test47_outy.html` == "") then
  echo "1FSP structure link missing"
  set result = 1
endif

# Check for correct chemical shift reference links in summary html file.
if (`grep -l 'make_uvd?pdbid=1FSP&file=test_data/20070228.28103.str&name=test47_out&do_csr=2&coord_index=1&csr_index=1' html_dir/uvd/test47_outy.html` == "") then
  echo "1FSP structure link missing"
  set result = 1
endif

if (`grep -l 'make_uvd?pdbid=1FSP&file=test_data/20070228.28103.str&name=test47_out&do_csr=2&coord_index=1&csr_index=2' html_dir/uvd/test47_outy.html` == "") then
  echo "1FSP structure link missing"
  set result = 1
endif

if (`grep -l 'make_uvd?pdbid=1FSP&file=test_data/20070228.28103.str&name=test47_out&do_csr=2&coord_index=1&csr_index=3' html_dir/uvd/test47_outy.html` == "") then
  echo "1FSP structure link missing"
  set result = 1
endif




echo -n "Test47 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
