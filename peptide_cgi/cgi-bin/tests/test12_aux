#! /bin/csh -f

# This simulates Dan Sinclair's chemShift software for running test12
# on a machine where we don't have access to the cross-mounted directories
# set up for chemShift on leech.


rm -f data_dir/vis_server/csr.run
cp test_data/csr1.1 data_dir/vis_server/test12_outcsr1_1.dat
cp test_data/csr1.2 data_dir/vis_server/test12_outcsr1_2.dat
cp test_data/csr1.3 data_dir/vis_server/test12_outcsr1_3.dat
cp test_data/csr1.4 data_dir/vis_server/test12_outcsr1_4.dat
cp test_data/csr1.5 data_dir/vis_server/test12_outcsr1_5.dat
touch data_dir/vis_server/test12_out.done
