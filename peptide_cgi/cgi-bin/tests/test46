#! /bin/csh -f

#TEMPTEMP -- test UVD

#TEMPTEMP -- should test PDB processing as separate step

#TEMPTEMP -- what about CSR???

#TEMPTEMP -- change this...
# Tests visualization-server mode -- local NMR-STAR file, but will try
# to get and process data from PDB.  Note: this NMR-STAR file is one
# assembled by Jafar from various uploaded pieces.

set result = 0

rm -f */test46_out*
rm -f */*/test46_out*

echo -n "Processing NMR-STAR: "
./uvd -file test_data/20070227.50906.str -name test46_out -force
if ($status != 0) then
  echo "First invocation of uvd failed!"
  set result = 1
endif


echo "Checking output files"

#TEMPTEMP -- make sure list is correct/complete
set files = ( data_dir/uvd/test46_outam1.dat \
	data_dir/uvd/test46_outamac1.dat \
	data_dir/uvd/test46_outas1.dat \
	data_dir/uvd/test46_outc1.dat \
