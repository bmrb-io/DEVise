  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

set file = "data_dir/4867ac1.dat"
set line = "1 1 G O5' O 118.098 -36.722 1.395 side_chains"
source tests/check4line
set line = "3444 175 GLY HA3 H 123.371 1.993 19.454 backbone"
