	html_dir/4096t22_large.html)

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end

set file = "data_dir/4096ac1.dat"
set line = "1 1 1 GLY N N 11.949 7.838 11.241 backbone"
source tests/check4line
set line = "2102 1 147 SER O2 O -11.593 18.226 3.842 side_chains"
source tests/check4line
set line = "2177 3 . . O O -7.051 -0.232 -1.921 backbone"
source tests/check4line

