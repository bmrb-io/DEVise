#! /bin/csh -f

# Hopefully final phase of torsion angle processing -- just process
# by using the PDB ID -- need to send request to the restraint grid
# and parse the result.

set result = 0

rm -f */16270*

echo -n "Processing NMR-STAR: "
./make_view number=16270
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif
mv tmp/s2d.out tmp/s2d.out1

echo -n "Processing mmCIF and torsion angle restraints: "
./make_view number=16270\&pdbid=2kid\&coord_index=2\&do_tar=3
if ($status != 0) then
  echo "Second invocation of s2d failed!"
  set result = 1
endif


source tests/test_tar2_check


echo -n "Test_tar2imv result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
