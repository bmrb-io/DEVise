#! /bin/csh -f

# 15117 has polypeptide and RNA.

set result = 0

rm -r -f */15117*

echo -n "Processing NMR-STAR and mmCIF: "
./s2d -nomail -name 15117 -file test_data/bmr15117_3.str -do_pdb 2 \
  -force -check_pct
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif

source tests/test62_check


# Output is slightly different for 2.1 and 3.0.
set file = "data_dir/15117md.dat"
set line = 'EA 1 (polypeptide(L))	1	C delta chem shift \[1\]	.	NS1A: 0.1 mM; ammonium acetate: 50 mM; ds-RNA: 0.2 mM	temperature: 293 K; pH: 6 pH; pressure: 1 atm	15117d1	\$C_DeltaShift$'
source tests/check4line
set line = 'EA 2 (polypeptide(L))	2	% 15N assign per res \[2\]	.	NS1A: 0.1 mM; ammonium acetate: 50 mM; ds-RNA: 0.2 mM	temperature: 293 K; pH: 6 pH; pressure: 1 atm	15117p2	\$assigForN$'
source tests/check4line


echo -n "Test62_3 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
