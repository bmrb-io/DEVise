#! /bin/csh -f

# Tests both BMRB and PDB processing with local files, so we can run this
# test without a web connection.

set result = 0

ln -s test_data/bmr4267.str .
ln -s test_data/1dfv.cif.gz .

rm -f */4267*

echo -n "Processing NMR-STAR: "
./s2d -nomail -bmrbid 4267 -local -force -check_res_list \
  -lacs_file test_data/bmr4267_LACS.str
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif

echo -n "Processing mmCIF: "
./s2d -nomail -pdbid 1dfv -bmrbid 4267 -do_pdb 2 -coord_index 1 -local -force \
  -check_res_list
if ($status != 0) then
  echo "Second invocation of s2d failed!"
  set result = 1
endif

rm bmr4267.str
rm 1dfv.cif.gz


echo "Checking output files"

set files = (data_dir/4267ac1.dat data_dir/4267as1.dat data_dir/4267c1.dat \
	data_dir/4267d1.dat data_dir/4267g1.dat data_dir/4267hn1.dat \
	data_dir/4267md.dat data_dir/4267n1.dat data_dir/4267n2.dat \
	data_dir/4267n3.dat data_dir/4267n4.dat data_dir/4267n5.dat \
	data_dir/4267n6.dat data_dir/4267n7.dat data_dir/4267n8.dat \
	data_dir/4267n9.dat data_dir/4267p1.dat data_dir/4267rc1.dat \
	data_dir/4267rl1.dat data_dir/4267t11.dat data_dir/4267t12.dat \
	data_dir/4267t13.dat data_dir/4267t21.dat data_dir/4267t22.dat \
	data_dir/4267t23.dat html_dir/4267ac1.html html_dir/4267as1.html \
	html_dir/4267c1.html html_dir/4267d1.html html_dir/4267g1.html \
	html_dir/4267hn1.html html_dir/4267n1.html html_dir/4267n2.html \
	html_dir/4267n3.html html_dir/4267n4.html html_dir/4267n5.html \
	html_dir/4267n6.html html_dir/4267n7.html html_dir/4267n8.html \
	html_dir/4267n9.html html_dir/4267p1.html html_dir/4267t11.html \
	html_dir/4267t12.html html_dir/4267t13.html html_dir/4267t21.html \
	html_dir/4267t22.html html_dir/4267t23.html \
	html_dir/4267y.html session_dir/4267ac1.ds session_dir/4267acdd \
	session_dir/4267as1.ds session_dir/4267c1.ds session_dir/4267d1.ds \
	session_dir/4267g1.ds session_dir/4267hn1.ds session_dir/4267n1.ds \
	session_dir/4267n2.ds session_dir/4267n3.ds session_dir/4267n4.ds \
	session_dir/4267n5.ds session_dir/4267n6.ds session_dir/4267n7.ds \
	session_dir/4267n8.ds session_dir/4267n9.ds session_dir/4267p1.ds \
	session_dir/4267t11.ds session_dir/4267t12.ds session_dir/4267t13.ds \
	session_dir/4267t21.ds session_dir/4267t22.ds session_dir/4267t23.ds \
	data_dir/4267am1.dat \
	data_dir/4267amac1.dat \
	html_dir/4267am1.html \
	session_dir/4267am1.ds \
	data_dir/4267lacsc1.dat \
	data_dir/4267lacsc2.dat \
	data_dir/4267lacsc3.dat \
	data_dir/4267lacsc4.dat \
	data_dir/4267lacsl1.dat \
	data_dir/4267lacsl2.dat \
	data_dir/4267lacsl3.dat \
	data_dir/4267lacsl4.dat \
	data_dir/4267lacsp1.dat \
	data_dir/4267lacsp2.dat \
	data_dir/4267lacsp3.dat \
	data_dir/4267lacsp4.dat \
	html_dir/4267lacs1.html \
	html_dir/4267lacs2.html \
	html_dir/4267lacs3.html \
	html_dir/4267lacs4.html \
	session_dir/4267lacs1.ds \
	session_dir/4267lacs2.ds \
	session_dir/4267lacs3.ds \
	session_dir/4267lacs4.ds)

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test1 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
