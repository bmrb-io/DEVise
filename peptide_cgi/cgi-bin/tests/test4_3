#! /bin/csh -f

# This is the same as test4, except on an NMR-STAR v3 file.

# Tests BMRB 4096 (a special case because it has coordinates).

set result = 0

rm -f */4096*

echo -n "Processing NMR-STAR: "
./s2d -nomail -name 4096 -file test_data/bmr4096_3.str -file test_data/bmr4038_3.str -force -check_res_list

if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif


source tests/test4_check


echo -n "Test4_3 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
