#! /bin/csh -f

# Tests chem shift reference processing (needs stuff on leech at BMRB
# to work right, unless -noleech argument is given -- then it does
# a "dummy" chemShift calculation).

set result = 0

set noleech = 0

if ($#argv > 0) then
  if (xx$1 == xx-noleech) then
    set noleech = 1
  endif
endif

rm -f */test6_out*
rm -f */*/test6_out*

if ($noleech) then
  tests/test6_noleech
  if ($status) then
    set result = 1
  endif
else
  tests/test6_main
  if ($status) then
    set result = 1
  endif
endif

echo "Checking output files"

set files = ( data_dir/vis_server/test6_outac1.dat \
	data_dir/vis_server/test6_outas1.dat \
	data_dir/vis_server/test6_outc1.dat \
	data_dir/vis_server/test6_outcsr1_1.dat \
	data_dir/vis_server/test6_outcsr1_2.dat \
	data_dir/vis_server/test6_outcsr1_3.dat \
	data_dir/vis_server/test6_outcsr1_4.dat \
	data_dir/vis_server/test6_outcsr1_5.dat \
	data_dir/vis_server/test6_outd1.dat \
	data_dir/vis_server/test6_outhc1.dat \
	data_dir/vis_server/test6_outhn1.dat \
	data_dir/vis_server/test6_outmd.dat \
	data_dir/vis_server/test6_outp1.dat \
	data_dir/vis_server/test6_outrc1.dat \
	data_dir/vis_server/test6_outrl.dat \
	html_dir/test6_outac1.html \
	html_dir/test6_outas1.html \
	html_dir/test6_outc1.html \
	html_dir/test6_outcsr11.html \
	html_dir/test6_outcsr21.html \
	html_dir/test6_outcsr31.html \
	html_dir/test6_outd1.html \
	html_dir/test6_outhc1.html \
	html_dir/test6_outhn1.html \
	html_dir/test6_outp1.html \
	html_dir/test6_outy.html \
	html_dir/test6_outy_large.html \
	session_dir/vis_server/test6_outac1.ds \
	session_dir/vis_server/test6_outacdd \
	session_dir/vis_server/test6_outas1.ds \
	session_dir/vis_server/test6_outc1.ds \
	session_dir/vis_server/test6_outcsr11.ds \
	session_dir/vis_server/test6_outcsr21.ds \
	session_dir/vis_server/test6_outcsr31.ds \
	session_dir/vis_server/test6_outd1.ds \
	session_dir/vis_server/test6_outhc1.ds \
	session_dir/vis_server/test6_outhn1.ds \
	session_dir/vis_server/test6_outp1.ds \
	data_dir/vis_server/test6_outam1.dat \
	html_dir/test6_outam1.html \
	session_dir/vis_server/test6_outam1.ds \
	html_dir/test6_outac1_large.html \
	html_dir/test6_outam1_large.html \
	html_dir/test6_outas1_large.html \
	html_dir/test6_outc1_large.html \
	html_dir/test6_outcsr11_large.html \
	html_dir/test6_outcsr21_large.html \
	html_dir/test6_outcsr31_large.html \
	html_dir/test6_outd1_large.html \
	html_dir/test6_outhc1_large.html \
	html_dir/test6_outhn1_large.html \
	html_dir/test6_outp1_large.html )

foreach file ($files)
  if (! -f $file) then
    echo "  Output file $file is missing"
    set result = 1
  endif
end


echo -n "Test6 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
