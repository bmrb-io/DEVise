#! /bin/csh -f

# Test an entry with DNA and protein.

set result = 0

rm -f */15083*

echo -n "Processing NMR-STAR and mmCIF: "
./s2d -nomail -name 15083 -file test_data/bmr15083_3.str -force -check_pct \
  -do_pdb 2
if ($status != 0) then
  echo "First invocation of s2d failed!"
  set result = 1
endif

source tests/test68_check


echo -n "Test68_3 result: "
if ($result == 0) then
  echo "OK"
  exit 0
else
  echo "FAILED"
  exit 1
endif
