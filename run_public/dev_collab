#! /bin/csh -f
# Run two DEVise client/server pairs, with collaborator.

# $Id$

setenv DEVISE_LIB /p/devise/public/lib
setenv DEVISE_COLLABORATOR `hostname`

\rm -f /tmp/Group.log
\rm -f /tmp/*master*
\rm -f /tmp/*server*

collaborator &

sleep 5

devised -port 6101 -switchport 6201 -imageport 6401 &
sleep 5
devisec -p 6101 &

devised -port 6102 &
sleep 5
devisec -p 6102 &
