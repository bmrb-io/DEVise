
mapping SolDnMapping AmsData {
	shape { RectXShape }
	static {
		float attrs[2];
		attrs[0] = 90.0; attrs[1] = 90.0;
		TDataMap::SetDefaultShape(SolDnMapping_RectXShapeID,2,attrs);
		TDataMap::SetDefaultColor(ColorMgr::AllocColor("blue"));
	}
	dynamic x,y,color {
		symbol->x = data->time;
		symbol->y = data->solDn;
	}
}
