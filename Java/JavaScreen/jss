#! /bin/csh -f

############################################################

# Start the jss (JavaScreen Server).

# Note: specific port number must NOT be specified in this script.

#-----------------------------------------------------------

# $Id$

############################################################

# The JSS can't manage to find DEVise.jspop and DEVise.kill unless
# '.' is in our path.
set path = ( . $path )

nohup ./jss_savepid $* &
