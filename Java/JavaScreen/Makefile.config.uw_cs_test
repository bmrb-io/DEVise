#-----------------------------------------------------------
# Directories for executables we need.
# Java 1.1.

# BIN2 is for JSS and devised.
JAVA_BIN2 = /usr/bin
PERL_BIN2 = /usr/bin
XVFB_BIN2 = /usr/bin

# BIN2P is for JSPoP.
JAVA_BIN2P = /usr/bin
PERL_BIN2P = /usr/bin

# BIN3 is for compiling.
JAVA_BIN3 = /usr/bin

POP_DIR = /scratch/devise/js.test
SERV_DIR = /scratch/devise/js.test
JAR_DIR = /var/home/www/public/html/devise/test
CGI_DIR = /var/home/www/public/cgi-bin/js_test
IDSTR = test

# Where the DEVise sessions, schemas, etc., are installed.
DEVISE_VIS = /p/devise/demo

# Data download setup.
DEVISE_JS_DATA_DOWNLOAD_DIR = /var/home/www/public/html/devise/test/data_download
DEVISE_JS_DATA_DOWNLOAD_URL = http://devise.cs.wisc.edu/devise/test/data_download/

# This is who gets email about any errors.
ERROR_EMAIL = devise@cs.wisc.edu

INSTALL_HOST = condor-websrv-7.cs.wisc.edu

# You should probably set this to 0 if you're running more than one JSS
# on the same machine.
KILL_XVFB = 0
