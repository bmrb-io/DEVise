schema test3_schema {
	delimiter = [\n]+;
	separator = [ 	]+ ;
	comment = "#";
	
	attr sci_const {
		type = double;
	}

	attr date1 {
		type = date;
		date_format = "MM/DD/YY hh:mm";
	}

	attr str1 {
		type = string;
		maxlen = 10;
		quote = ['];
		separator = [ 	]+ ;
	}

	attr last {
		separator = [%]+;
		type = int;
	}
}
