CC = g++

CFLAGS = $(ARCH_FLAGS) -O -Wall -I../../graphics.new
LDFLAGS = $(ARCH_LDFLAGS) $(ARCH_LDPOST)

.C.o:
	$(CC) -c $(CFLAGS) $<

SRC = http_url.c

OBJ = $(SRC:.c=.o)

all: http_url

http_url: $(OBJ)
	$(CC) -o $@ $(OBJ) $(LDFLAGS)

.PHONY: clean mostlyclean

clean: mostlyclean
	-rm http_url

mostlyclean:
	-rm $(OBJ)
