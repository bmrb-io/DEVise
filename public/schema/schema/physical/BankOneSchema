schema BankOneSchema {
	type = text ;
	comment = '//' ;
	whitespace = ' 	' ;


	attr Date {
		type = datetime ;
		date_format = '%Y-%m-%d' ;
	}

	attr Account {
		type = string ;
		maxlen = 10 ;
	}

	attr Amount {
		type = double ;
	}

}
