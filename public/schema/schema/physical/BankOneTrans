schema BankOneSchema {
	type = text ;
	comment = '//' ;
	whitespace = ' 	' ;


	attr FromAccount {
		type = string ;
		maxlen = 10 ;
	}

	attr Control1 {
		type = int ;
	}

	attr ToAccount {
		type = string ;
		maxlen = 10 ;
	}

	attr Control2 {
		type = int ;
	}


	attr Amount {
		type = double ;
	}

	attr Date {
		type = datetime ;
		date_format = '%Y-%m-%d' ;
	}


}
