CPPFLAGS = $(ARCH_FLAGS) -O -I../../graphics.new -I../../tape \
	-Wall -Wno-unused -fexternal-templates

LIBSRC = binconv.C extract.C rectape.C

LIBOBJ = $(LIBSRC:.C=.o)

all: issmobj.o

issmobj.o: $(LIBOBJ)
	ld -r -o $@ $(LIBOBJ)

.PHONY: clean mostlyclean

clean: mostlyclean
	-rm issmobj.o

mostlyclean:
	-rm $(LIBOBJ)
