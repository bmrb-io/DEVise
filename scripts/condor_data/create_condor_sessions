#! /bin/csh -f
rm -r userdat

set dataFile = "UserLastMonth.dat"
if (-f $dataFile) then
  rm $dataFile
endif

/p/condor/home/bin/condor_devise_userdata -lastweek > UserLastMonth.dat

split_user_data UserLastMonth.dat userdat

if (-f $dataFile) then
  rm $dataFile
endif

create_2users userdat UserMonth.ds \
  /p/devise/demo/schema/schema/physical/Condor:userlist \
  /p/devise/demo/schema/schema/physical/Condor:user1

create_user_thumbnails userdat UserThumb.ds \
  /p/devise/demo/schema/schema/physical/Condor:userlist \
  /p/devise/demo/schema/schema/physical/Condor:user1
