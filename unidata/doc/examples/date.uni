schema date {

    type = text;
    comment = '//';
    whitespace = ' \t';

    attr month {
        type = int;
    }

    attr day {
        type = int;
    }

    attr year {
        type = int;
    }

    attr date {
        type = datetime;
        value = ("year", $year, "month", $month, "day", $day);
    }
}
