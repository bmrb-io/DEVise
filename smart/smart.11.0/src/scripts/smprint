#! /bin/csh
# Print the given relation

set bin = BIN
set spec_file = TLIBDIR/spec.default
set begin_id = 0
set end_id = 2147483647

if ($#argv < 2) then
        echo 'Usage: smprint [-b begin_id] [-e end_id] [-s spec_file] objtype filename'
        exit
endif

while ("$1" == "-b" || "$1" == "-e" || "$1" == "-s")
        switch ($1)
                case "-b":
                        set begin_id = $2
                        shift
                        shift
                        breaksw
                case "-e":
                        set end_id = $2
                        shift
                        shift
                        breaksw
                case "-s":
                        set spec_file = $2
                        shift
                        shift
                        breaksw
        endsw
end

if ($#argv < 2) then
        echo 'Usage: smprint [-b begin_id] [-e end_id] [-s spec_file] objtype filename'
        exit
endif

set objtype = $1
set filename = ($argv[2-])

$bin/smart print $spec_file global_start $begin_id    global_end $end_id \
                       proc print.obj.$objtype   in "$filename"
