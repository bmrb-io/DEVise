CPPFLAGS = $(ARCH_FLAGS) -Wall -Wno-unused -O -DUNIX  -I../../graphics \
	-I../../graphics.new -fexternal-templates

LIBSRC = tapedrive.C QueryProcTape.C Templates.C

LIBOBJ = $(LIBSRC:.C=.o)

all: tapeobj.o

tapeobj.o: $(LIBOBJ)
	ld -r -o $@ $(LIBOBJ)

.PHONY: clean mostlyclean

clean: mostlyclean
	-rm tapeobj.o

mostlyclean:
	-rm $(LIBOBJ)
