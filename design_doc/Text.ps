1 setlinewidth
/objectdict 4 dict def
/printtiled
{
  /rows exch def
  /columns exch def
  /bigpictureproc exch def
  1 -1 scale
  newpath
    leftmargin botmargin neg moveto
    0 pageheight neg rlineto
    pagewidth 0 rlineto
    0 pageheight rlineto
  closepath clip

  leftmargin botmargin neg translate

  1 1 rows
  {
    /rowcount exch def
    0 1 columns 1 sub
    {
      /colcount exch def
      gsave
	pagewidth colcount mul neg
	pageheight rowcount mul neg
	translate
	bigpictureproc
	gsave showpage grestore
      grestore
    } for
  } for
} def
/drawbox
{
  /height exch def
  /posy exch def
  /posx exch def
  newpath
    posx posy moveto
    0 height  rlineto
    rectwidth 0  rlineto
    0 0 height sub rlineto
  closepath
  gsave
   1 setgray fill
  grestore
  stroke
} def
/drawNode
{
  gsave
  /height exch def
  /width exch def
  /posy exch def
  /posx exch def
  /oname exch def
  /boxheight height 3 div def
  newpath
    posx posy moveto 0 height rlineto width 0 rlineto 0 0 height sub rlineto
  closepath
  gsave
    .9 setgray fill
  grestore
  posx posy boxheight add boxheight drawbox
  posx textxoffset add rectwidth add posy height add textyoffset sub moveto
  gsave  1 -1 scale oname show grestore
  posx width add rectwidth sub posy boxheight drawbox
  grestore
} def
/drawLink
{
  /toheight exch def
  /toy exch def
  /tox exch def
  /fromheight exch def
  /fromy exch def
  /fromx exch def
  fromx rectwidth 2 div sub fromy fromheight 2 div add moveto
  tox rectwidth 2 div add toy toheight 2 div add lineto
  stroke
} def
